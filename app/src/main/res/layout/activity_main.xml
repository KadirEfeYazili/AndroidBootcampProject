        android:textColor="@color/yaziRenk1"
        android:textSize="50sp"
        android:textStyle="bold"
        app:chipBackgroundColor="@color/anaRenk"
        app:chipCornerRadius="@android:dimen/app_icon_size"
        app:chipMinHeight="18dp"
        app:chipMinTouchTargetSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tus8"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tus4" />

    <com.google.android.material.chip.Chip
        android:id="@+id/tus4"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginStart="32dp"
