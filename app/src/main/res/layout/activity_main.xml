<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="#F51A1818"
    tools:context=".MainActivity">

    <View
        android:id="@+id/görüntü"
        android:layout_width="412dp"
        android:layout_height="465dp"
        android:layout_marginTop="60dp"
        android:background="#F51F1B1B"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textViewsonuc"
        tools:layout_editor_absoluteX="-1dp" />

    <com.google.android.material.chip.Chip
        android:id="@+id/tus9"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginTop="504dp"
        android:layout_marginEnd="20dp"
        android:buttonTint="#FFFFFF"
        android:drawableTintMode="multiply"
        android:shadowColor="@color/anaRenk"
        android:text="9"
        android:textColor="@color/yaziRenk1"
        android:textSize="50sp"
        android:textStyle="bold"
        app:checkedIconEnabled="false"
        app:chipBackgroundColor="@color/anaRenk"
        app:chipCornerRadius="@android:dimen/app_icon_size"
        app:chipIconVisible="false"
        app:chipMinHeight="18dp"
        app:chipMinTouchTargetSize="16dp"
        app:iconEndPadding="0dp"
        app:iconStartPadding="0dp"
        app:layout_constraintEnd_toStartOf="@+id/tussil"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <com.google.android.material.chip.Chip
        android:id="@+id/tus8"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginTop="504dp"
        android:layout_marginEnd="35dp"
        android:buttonTint="#FFFFFF"
        android:drawableTintMode="multiply"
        android:shadowColor="@color/anaRenk"
        android:text="8"
        android:textColor="@color/yaziRenk1"
        android:textSize="50sp"
        android:textStyle="bold"
        app:chipBackgroundColor="@color/anaRenk"
        app:chipCornerRadius="@android:dimen/app_icon_size"
        app:chipIconEnabled="false"
        app:chipMinHeight="18dp"
        app:chipMinTouchTargetSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tus9"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <com.google.android.material.chip.Chip
        android:id="@+id/tus2"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginTop="336dp"
        android:layout_marginEnd="35dp"
        android:buttonTint="#FFFFFF"
        android:drawableTintMode="multiply"
        android:shadowColor="@color/anaRenk"
        android:text="2"
        android:textColor="@color/yaziRenk1"
        android:textSize="50sp"
        android:textStyle="bold"
        app:chipBackgroundColor="@color/anaRenk"
        app:chipCornerRadius="@android:dimen/app_icon_size"
        app:chipIconEnabled="false"
        app:chipMinHeight="18dp"
        app:chipMinTouchTargetSize="16dp"
        app:layout_constraintEnd_toStartOf="@+id/tus3"
        app:layout_constraintTop_toBottomOf="@+id/toolbar" />

    <com.google.android.material.chip.Chip
        android:id="@+id/tus5"
        android:layout_width="63dp"
        android:layout_height="55dp"
        android:layout_marginTop="420dp"
        android:layout_marginEnd="35dp"
        android:buttonTint="#FFFFFF"
        android:drawableTintMode="multiply"
        android:shadowColor="@color/anaRenk"
